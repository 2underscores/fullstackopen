{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch via NPM",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "env": {
                "NODE_ENV": "development"
            },
            "cwd": "${workspaceFolder}/library-be",
            "console": "integratedTerminal"
        }
    ]
}